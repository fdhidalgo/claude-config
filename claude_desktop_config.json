{
  "globalShortcut": "Alt+Ctrl+Cmd+C",
  "mcpServers": {
    "chrome-devtools": {
      "command": "npx",
      "args": [
        "chrome-devtools-mcp@latest"
      ]
    },
    "mcp-ical": {
      "command": "uv",
      "args": [
        "--directory",
        "/Users/dhidalgo/mit/Apps/mcp-ical/mcp-ical",
        "run",
        "mcp-ical"
      ]
    },
    "Monarch Money": {
      "command": "/opt/homebrew/bin/uv",
      "args": [
        "run",
        "--directory",
        "/Users/dhidalgo/Documents/personal_scripts/monarch-mcp-server",
        "python",
        "-m",
        "monarch_mcp_server.server"
      ],
      "env": {
        "PYTHONPATH": "/Users/dhidalgo/Documents/personal_scripts/monarch-mcp-server/src"
      }
    },
    "obsidian-mcp-server": {
      "command": "npx",
      "args": [
        "obsidian-mcp-server"
      ],
      "env": {
        "OBSIDIAN_API_KEY": "${OBSIDIAN_API_KEY}",
        "OBSIDIAN_BASE_URL": "http://127.0.0.1:27123",
        "OBSIDIAN_VERIFY_SSL": "false",
        "OBSIDIAN_ENABLE_CACHE": "true"
      },
      "disabled": false,
      "autoApprove": []
    }
  },
  "preferences": {
    "quickEntryDictationShortcut": "capslock"
  }
}
